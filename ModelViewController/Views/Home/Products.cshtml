@model IEnumerable<ModelViewController.Models.Product>

@{
    ViewData["Title"] = "Products";
}

<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-4">
                <partial name="_ProductsList" model="item" />
            </div>   
        }
    </div>
</div>

    